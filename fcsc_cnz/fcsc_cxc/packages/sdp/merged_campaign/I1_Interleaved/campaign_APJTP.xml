      <procedurePeriod saSmfProcPeriod="60000000000"/>
    </outageInfo>
    <upgradeMethod>
      <singleStepUpgrade>
        <upgradeScope>
          <forAddRemove>
            <deactivationUnit/>
            <activationUnit>
              <swAdd bundleDN="safSmfBundle=ERIC-ACS_APJTP-CXC1371324-R1A08" pathnamePrefix="/home/coremw/repository">
                <plmExecEnv amfNode="safAmfNode=SC-1,safAmfCluster=myAmfCluster"/>
                <!-- Remove next line if only one SC on the system -->
                <plmExecEnv amfNode="safAmfNode=SC-2,safAmfCluster=myAmfCluster"/>
              </swAdd>
            </activationUnit>
          </forAddRemove>
        </upgradeScope>
        <upgradeStep/>
      </singleStepUpgrade>
    </upgradeMethod>
  </upgradeProcedure>
   <campaignWrapup>
