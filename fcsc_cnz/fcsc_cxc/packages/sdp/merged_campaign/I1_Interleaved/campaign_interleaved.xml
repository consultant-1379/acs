        <plmExecEnv amfNode="safAmfNode=SC-1,safAmfCluster=myAmfCluster"/>
</procInitAction>
    <upgradeMethod>
      <singleStepUpgrade>
        <upgradeScope>
          <forAddRemove>
            <deactivationUnit/>
            <activationUnit>
              <swAdd bundleDN="safSmfBundle=ERIC-ACS_APCONF-CXC1371346-R1A05" pathnamePrefix="/home/coremw/repository">
			   <plmExecEnv amfNode="safAmfNode=SC-1,safAmfCluster=myAmfCluster"/>
                <plmExecEnv amfNode="safAmfNode=SC-2,safAmfCluster=myAmfCluster"/>
			   </swAdd>
			  <swAdd bundleDN="safSmfBundle=ERIC-ACS_APJTP-CXC1371324-R1A08" pathnamePrefix="/home/coremw/repository">
                <plmExecEnv amfNode="safAmfNode=SC-1,safAmfCluster=myAmfCluster"/>
                <plmExecEnv amfNode="safAmfNode=SC-2,safAmfCluster=myAmfCluster"/>
              </swAdd>
            </activationUnit>
          </forAddRemove>
        </upgradeScope>
        <upgradeStep/>
      </singleStepUpgrade>
    </upgradeMethod>
  </upgradeProcedure>
 <campaignWrapup>
    <waitToCommit/>
    <waitToAllowNewCampaign/>
    <removeFromImm/>
 </campaignWrapup>
</upgradeCampaign>
