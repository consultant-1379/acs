<?xml version="1.0" encoding="utf-8"?>
<upgradeCampaign xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  safSmfCampaign="safSmfCampaign=ERIC-ACS-I1-TEMPLATE-R1A01">
  <campaignInfo>
    <campaignPeriod/>
  </campaignInfo>
  <campaignInitialization>
    <addToImm/>
  </campaignInitialization>
  <upgradeProcedure safSmfProcedure="safSmfProc=SingleStepProc">
    <outageInfo>
      <acceptableServiceOutage>
        <all/>
      </acceptableServiceOutage>
      <procedurePeriod saSmfProcPeriod="60000000000"/>
    </outageInfo>
<procInitAction>
	<doCliCommand command="cmw-addToImm" args="ERIC-ACS_APCONF-CXC1371346-R1A05"/>
